{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "0.1",
  "default_locale": "zh_CN",
  "description": "__MSG_extDesc__",
  
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },

  "permissions": [
    "storage",
    "tabs"
  ],

  "action": {
    "default_title": "__MSG_actionTitle__"
  },

  "icons": {
    "128": "icon/extensionicon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["icon/*"],
      "matches": ["<all_urls>"]
    }
  ]
}