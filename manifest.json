{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "0.1.1",
  "default_locale": "zh_CN",
  "description": "__MSG_extDesc__",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs"
  ],
  "host_permissions": [
    "https://www.google.com/*",
    "*://*/*"
  ],
  "action": {
    "default_title": "__MSG_actionTitle__"
  },
  "icons": {
    "128": "icon/extensionicon.png",
    "256": "icon/extensionicon256.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icon/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}